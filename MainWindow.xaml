<Window x:Class="ZeroCup.DataHelper.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZeroCup.DataHelper"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <Button
            x:Name="LoadFromFileButton"
            Click="LoadDataFromFileButton_Click"
            Content="Load Data From File"/>
        <Button
            x:Name="FolderButton"
            Click="FolderButton_Click"
            Content="Select Folder"/>
        <Button
            x:Name="RenameButton"
            Click="RenameButton_Click"
            Content="Rename Images"/>
        <Button
            x:Name="LoadDataButton"
            Click="LoadDataButton_Click"
            Content="Load Data"/>
        <Button
            x:Name="SaveDataButton"
            Click="SaveDataButton_Click"
            Content="Save Data Class"/>
    </StackPanel>
</Window>
